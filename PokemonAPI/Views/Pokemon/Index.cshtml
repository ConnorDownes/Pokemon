@model PokemonAPI.Models.PokemonBasic.PokemonBasic

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.results[0].name)
            </th>
            <th>

            </th>
        </tr>
    
    @foreach (var item in Model.results) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.ActionLink("View", "Details", new { name = item.name })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
